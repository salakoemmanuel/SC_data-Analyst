-- Database: `nigeria_lga_districts`

-- Table structure for table `lga_tbl`


CREATE TABLE `lga_tbl` (
  `lga_id` INT(11) NOT NULL,
  `state_id` INT(11) DEFAULT NULL,
  `senatorial_district_id` INT(11) DEFAULT NULL,
  `representative_district_id` INT(11) DEFAULT NULL,
  `lga_name` VARCHAR(50) COLLATE utf8_unicode_ci NOT NULL
) ENGINE=INNODB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

